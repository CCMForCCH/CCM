<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.cohoman.model.integration.persistence.beans.TrashCycleBean" table="trashcycles" >
  
  	  <id name="trashcycleid">
		 <generator class="increment" />
	  </id>

	  <property name="trashcyclestartdate" />
	  <property name="trashcycleenddate" />
	  <property name="lastuserskipped" />
	  <property name="lastunitskipped" />
	  <property name="nextusertoskip" />
	  
  </class>
  
</hibernate-mapping>