<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.cohoman.model.integration.persistence.beans.MtaskBean" table="mtask" >
  
  	  <id name="mtaskitemid">
		 <generator class="increment" />
	  </id>

	  <property name="maintenanceitemid" />
	  <property name="vendorname" />
	  <property name="taskstartDate" />
	  <property name="taskendDate" />
	  <property name="itemCreatedBy" />
	  <property name="notes" />
	  
  </class>
    
</hibernate-mapping>