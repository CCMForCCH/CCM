<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.cohoman.model.integration.persistence.beans.MaintenanceBean" table="maintenance" >
  
  	  <id name="maintenanceitemid">
		 <generator class="increment" />
	  </id>

	  <property name="itemname" />
	  <property name="itemdescription" />
	  <property name="itemCreatedDate" />
	  <property name="itemCreatedBy" />
	  <property name="priority" />
	  <property name="frequencyOfItem" />
	  <property name="lastperformedDate" />
	  <property name="targetTimeOfyear" />
	  
  </class>
    
</hibernate-mapping>